visited = []

def pi(numPie, numPeople, min):
  if visited [numPie][numPeople][min] == 0:
    if numPie == numPeople:
        visited[numPie][numPeople][min] = 1
    elif numPeople == 1:
        visited[numPie][numPeople][min] = 1
    else:
      t = 0
      for i in range (min, int((numPie / numPeople) + 1)):
          t = t + pi(numPie - i, numPeople - 1, i)
      visited[numPie][numPeople][min] = t
  return visited[numPie][numPeople][min]



numPie = int(input())
numPeople = int(input())

for i in range(numPie + 1):
  map1 = []
  for j in range(numPie + 1):
    map2 = []
    for k in range(numPie + 1):
      map2.append (0)
    map1.append(map2)
  visited.append(map1)

print (pi(numPie, numPeople, 1))
